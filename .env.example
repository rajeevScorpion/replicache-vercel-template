# Replicache License Key
# Get a dev license key from: https://replicache.dev/license
NEXT_PUBLIC_REPLICACHE_LICENSE_KEY=your_replicache_license_key_here

# Supabase Configuration (Required for Replicache)
# Get these from your Supabase project settings > API
NEXT_PUBLIC_SUPABASE_URL=https://[YOUR-PROJECT-REF].supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Database password from Supabase settings > Database
SUPABASE_DATABASE_PASSWORD=your_database_password_here